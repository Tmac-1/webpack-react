.todoList {
  .box{
    float: left;
    width: 300px;
  }
  li {
    list-style-type: none;
    margin-top: 10px;
  }
  button {
    border: none;
    border-radius: 4px;
    font-size: 12px;
    padding: 2px 10px;
    margin-left: 10px;
  }
  .del{
    background-color: #f00000;
    color: #FFFFFF;
  }
  .recovery{
    background-color: chartreuse;
  }
}